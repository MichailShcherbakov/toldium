{
  "name": "toldium-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "apollo:compile": "graphql-codegen -c apollo.codegen.ts",
    "apollo:watch": "graphql-codegen -c apollo.codegen.ts -w",
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@fontsource/noto-sans": "^4.5.11",
    "@vue/apollo-composable": "^4.0.0-beta.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.11.3",
    "lodash.sortby": "^4.7.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash.sortby": "^4.7.7",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
